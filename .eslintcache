n  \n  // Return the id of the stock to then retrieve user holdings for the stock and pass it to the \n  // UpdateHoldings component as a prop\n  const stockId = stockHoldings[0].stock_id;\n\n  return (\n    <div>\n      <h1>{stock.companyName}</h1>\n      <CompanyDetails symbol={symbol} />\n      <CompanyInformation \n        companyName={stock.companyName}\n        symbol={stock.symbol}\n        price={stock.latestPrice}\n        marketCap={stock.marketCap}\n        pe={stock.peRatio}\n      />\n      <UpdateHoldings stock={stock} holdings={stockHoldings} stockId={stockId} />\n      <StockNews />\n    </div>\n  )\n}\n\nexport default StockPage\n","/Users/Charlie_Crane/code/charc46/investment-portfolio-app/finance-portfolio-tracker/src/components/pages/Holdings/Holdings.js",["108"],"/Users/Charlie_Crane/code/charc46/investment-portfolio-app/finance-portfolio-tracker/src/components/pages/Dashboard/PortfolioOverview.js",[],"/Users/Charlie_Crane/code/charc46/investment-portfolio-app/finance-portfolio-tracker/src/components/pages/Dashboard/TopSectors.js",[],"/Users/Charlie_Crane/code/charc46/investment-portfolio-app/finance-portfolio-tracker/src/components/pages/Dashboard/TopMarkets.js",[],"/Users/Charlie_Crane/code/charc46/investment-portfolio-app/finance-portfolio-tracker/src/components/pages/Stock/CompanyDetails.js",["109"],"import React, { useState, useEffect} from 'react'\nimport axios from 'axios'\n\nconst CompanyDetails = ({symbol}) => {\n  const [company, setCompany] = useState({})\n\n  useEffect(() => {\n    const url = `https://sandbox.iexapis.com/stable/stock/${symbol}/company?`;\n    const getCompanyDetails = async () => {\n      const { data } = await axios.get(url, {\n        params: {\n          token: 'Tpk_08b5a20b77b74bce8c14e491f5bc75f1'\n        }\n      })\n    setCompany(data);\n    }\n\n    getCompanyDetails();\n  }, [])\n\n  return (\n    <div className='ui segment'>\n      <h2>Company Information for {company.companyName}</h2>\n      <p>Industry: {company.industry}</p>\n      <p>Description: {company.description}</p>\n      <p>CEO: {company.CEO}</p>\n      <p>Sector: {company.sector}</p>\n      <a href={company.website}>Website: {company.website}</a>\n    </div>\n  )\n}\n\nexport default CompanyDetails\n","/Users/Charlie_Crane/code/charc46/investment-portfolio-app/finance-portfolio-tracker/src/components/pages/Stock/CompanyInformation.js",["110","111"],"import React from 'react'\nimport CompanyDetails from './CompanyDetails'\n\nconst CompanyInformation = (props) => {\n  const symbol = props.symbol\n\n  return (\n    <div className='ui segment'>\n      <div className='financials'>\n        <h2>Financial Information for {props.companyName}</h2>\n        <p>Ticker: {props.symbol}</p>\n        <p>Price: {props.price}</p>\n        <p>Market Cap: {props.marketCap}</p>\n        <p>P/E Ratio: {props.pe}</p>\n      </div>\n    </div>\n  )\n}\n\nexport default CompanyInformation\n","/Users/Charlie_Crane/code/charc46/investment-portfolio-app/finance-portfolio-tracker/src/components/pages/Stock/StockNews.js",[],"/Users/Charlie_Crane/code/charc46/investment-portfolio-app/finance-portfolio-tracker/src/components/pages/Dashboard/TopHoldings.js",["112"],"/Users/Charlie_Crane/code/charc46/investment-portfolio-app/finance-portfolio-tracker/src/components/pages/Dashboard/Chart.js",[],"/Users/Charlie_Crane/code/charc46/investment-portfolio-app/finance-portfolio-tracker/src/components/pages/Stock/NewsSnippet.js",[],"/Users/Charlie_Crane/code/charc46/investment-portfolio-app/finance-portfolio-tracker/src/components/pages/Holdings/UpdateHoldings.js",["113"],"import React, {useState, useEffect} from 'react'\nimport axios from 'axios'\n\nconst UpdateHoldings = ({stock, stockId}) => {\n  const [units, setUnits] = useState(null);\n  const [cost, setCost] = useState(null);\n\n  useEffect(() => {\n    // Calling rails API for all users holdings which match the stockId passed in with props,\n    // this returns an object with holding information including date, price, stock ticker, \n    // stock id from API \n    const url = `http://localhost:3001/api/v1/users/holdings/${stockId}`;\n    const getHolding = async () => {\n      const { data } = await axios.get(url)\n      setUnits(data.holdings_total);\n      setCost(data.holdings_cost);\n    }\n    getHolding();\n  }, [])\n\n  return (\n    <div className='ui segment'>\n      <h2>Update Holdings for {stock.companyName}</h2>\n      <div className='current-holdings'>\n        <h3>Current holdings: {units}</h3>\n        <h3>Current holdings cost: {cost}</h3>\n      </div>\n    </div>\n  )\n}\n\nexport default UpdateHoldings\n","/Users/Charlie_Crane/code/charc46/investment-portfolio-app/finance-portfolio-tracker/src/reducers/index.js",[],"/Users/Charlie_Crane/code/charc46/investment-portfolio-app/finance-portfolio-tracker/src/actions/index.js",[],"/Users/Charlie_Crane/code/charc46/investment-portfolio-app/finance-portfolio-tracker/src/reducers/topHoldingsReducer.js",["114"],"export default (state = [], action) => {\n  switch(action.type) {\n    case 'FETCH_TOP_HOLDINGS':\n      return action.payload;\n    default:\n      return state;\n  }\n}","/Users/Charlie_Crane/code/charc46/investment-portfolio-app/finance-portfolio-tracker/src/reducers/holdingsReducer.js",["115"],"/Users/Charlie_Crane/code/charc46/investment-portfolio-app/finance-portfolio-tracker/src/reducers/groupedHoldingsReducer.js",["116"],{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","replacedBy":"120"},{"ruleId":"121","severity":1,"message":"122","line":21,"column":6,"nodeType":"123","endLine":21,"endColumn":8,"suggestions":"124"},{"ruleId":"117","replacedBy":"125"},{"ruleId":"119","replacedBy":"126"},{"ruleId":"127","severity":1,"message":"128","line":11,"column":10,"nodeType":"129","messageId":"130","endLine":11,"endColumn":19},{"ruleId":"121","severity":1,"message":"131","line":26,"column":6,"nodeType":"123","endLine":26,"endColumn":8,"suggestions":"132"},{"ruleId":"121","severit